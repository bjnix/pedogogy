 */
public class AdjListGraph<V, E> implements Graph<V, E> {
	
	
	private class vertEntry implements Vertex<E>
	{
		private UnorderedMap<Object,Object> vmap = new UnorderedMap<Object,Object>();
		@Override
		public E element() {
			return null;
		}

		@Override
		public Iterable<Entry<Object, Object>> entries() {
			return vmap.entries() ;
		}

		@Override
		public Object get(Object key) throws InvalidKeyException {
			return vmap.get(key);
		}

		@Override
		public boolean isEmpty() {
			return vmap.isEmpty();
		}

		@Override
		public Iterable<Object> keys() {
			return vmap.keys();
		}

		@Override
		public Object put(Object key, Object value) throws InvalidKeyException {
			return vmap.put(key, value);
		}

		@Override
		public Object remove(Object key) throws InvalidKeyException {
			return vmap.remove(key);
		}

		@Override
		public int size() {
			return vmap.size();
		}

		@Override
		public Iterable<Object> values() {
			return vmap.values();
		}
		
	}
	private class edgeEntry implements Edge<E>
	{
		private UnorderedMap<Object, Object> emap = new un;

		@Override
		public E element() {
			// TODO Auto-generated method stub
			return null;
		}

		@Override
		public Iterable<Entry<Object, Object>> entries() {
			return emap.entries();
		}

		@Override
		public Object get(Object key) throws InvalidKeyException {
			return emap.get(key);
		}

		@Override
		public boolean isEmpty() {
			return emap.isEmpty();
		}

		@Override
		public Iterable<Object> keys() {
			return emap.keys();
		}

		@Override
		public Object put(Object key, Object value) throws InvalidKeyException {
			return emap.put(key, value);
		}

		@Override
		public Object remove(Object key) throws InvalidKeyException {
			return emap.remove(key);
		}

		@Override
		public int size() {
			return emap.size();
		}

		@Override
		public Iterable<Object> values() {
			return emap.values();
		}
		
	}

